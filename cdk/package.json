{
  "name": "cloudfront-s3-cdn",
  "version": "0.1.0",
  "bin": {
    "cognito": "bin/cloudfront-s3-cdn.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "format-check": "rome format bin lib",
    "format": "rome format --write bin lib",
    "check": "rome check bin lib",
    "check-fix": "rome check --apply-suggested bin lib",
    "lint": "pnpm format-check && pnpm check",
    "lint-fix": "pnpm format && pnpm check-fix",
    "clean": "rimraf build",
    "prelint": "pnpm install",
    "prelint-fix": "pnpm install",
    "prebuild": "pnpm lint"
  },
  "devDependencies": {
    "@swc/cli": "*",
    "@swc/core": "*",
    "@swc/helpers": "*",
    "@types/node": "*",
    "@types/prettier": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "aws-cdk": "*",
    "eslint": "*",
    "eslint-config-airbnb-base": "*",
    "eslint-config-airbnb-typescript": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-prettier": "*",
    "prettier": "*",
    "regenerator-runtime": "*",
    "rome": "*",
    "ts-node": "*",
    "typescript": "*"
  },
  "dependencies": {
    "aws-cdk-lib": "*",
    "constructs": "*",
    "esbuild": "*",
    "source-map-support": "*"
  },
  "resolutions": {
    "got": ">=11.8.5",
    "json5": ">=2.2.3",
    "semver-regex": ">=3.1.3"
  }
}
