{
  "name": "cloudfront-s3-cdn",
  "version": "0.1.0",
  "bin": {
    "cognito": "bin/cloudfront-s3-cdn.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "format-check": "rome format bin lib",
    "format": "rome format --write bin lib",
    "check": "rome check bin lib",
    "check-fix": "rome check --apply-suggested bin lib",
    "lint": "pnpm format-check && pnpm check",
    "lint-fix": "pnpm format && pnpm check-fix",
    "clean": "rimraf build",
    "prelint": "pnpm install",
    "prelint-fix": "pnpm install",
    "prebuild": "pnpm lint"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.61",
    "@swc/core": "^1.3.32",
    "@swc/helpers": "^0.4.14",
    "@types/node": "^18.11.18",
    "aws-cdk": "^2.63.1",
    "regenerator-runtime": "^0.13.11",
    "rome": "^11.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.63.1",
    "constructs": "^10.1.240",
    "esbuild": "^0.17.5",
    "source-map-support": "^0.5.21"
  },
  "resolutions": {
    "got": ">=11.8.5",
    "json5": ">=2.2.3",
    "semver-regex": ">=3.1.3"
  }
}
